<% if(success && success.length){ %> 
    <div class="flash-msg flash-msg-success" role="alert">
        <%= success %> 
    </div>
<% } %> 
<% if(error && error.length){ %> 
    <div class="flash-msg flash-msg-error" role="alert">
        <%= error %> 
    </div>
    <% console.log(error) %> 
<% } %> 